<h1>Customers</h1>

<table>
  <thead>
    <tr>
      <th>Customer Name</th>
      <th>Account Number</th>
      <th>Web URL</th>
    </tr>
  </thead>

  <tbody>
    <% @customers.each do |customer| %>
      <tr>
        <td><%= customer.name %> </td>
        <td><%= customer.acct_numbner %></td>
        <td><%= link_to customer.url, "#{customer.url}" %></td>

      </tr>
    <% end %>
  </tbody>
</table>